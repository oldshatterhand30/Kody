#include <stdio.h>

int main(){
	int x, y;
	char znak;
	
	scanf("%d %c %d", &x, &znak, &y);
	
	if(znak == '+')	printf("%d\n", x + y);
	else	if(znak == '-')	printf("%d\n", x - y);
		else	if(znak == '*')	printf("%d\n", x * y);
			else	if(znak == '/'){	
					if(y != 0)	printf("%d\n", x / y);
					else		printf("Operacja niedozwolona. y = 0. Nie mozna dzielic przez 0.\n");
				}
				else printf("Podano bledny znak\n");
	return 0;
}
